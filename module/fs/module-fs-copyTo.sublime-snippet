fs.copyTo({
    oldPath: '${1:old_path}',
    newPath: '${2:new_path}'
},function(ret,err){
    if (ret.status) {
        api.alert({msg:'拷贝文件成功'});
    }else {
        api.alert({msg:err.msg});
    }
});
