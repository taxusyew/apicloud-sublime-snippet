wx.shareText({
    apiKey: '${1:apikey}',
    scene: '${2:scene_type}',
    text: '${3:share_text}'
}, function(ret, err){
    if(ret.status){
        alert("分享成功");
    }else{
        alert(err.code);
    }
});
